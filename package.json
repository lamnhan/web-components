{
  "name": "app",
  "version": "0.0.0",
  "description": "A standalone components built with Vue, Lit and Native.",
  "scripts": {
    "dev:native": "parcel consume/native/index.html --dist-dir www",
    "build:native": "npm run build:native-standalone",
    "build:native-standalone": "parcel build ./src/native/input.ts --dist-dir build/native/standalone",
    "dev:lit": "parcel consume/lit/index.html --dist-dir www",
    "build:lit": "npm run build:lit-standalone",
    "build:lit-standalone": "parcel build ./src/lit/input.ts --dist-dir build/lit/standalone",
    "build:vue": "npm run build:vue-standalone",
    "build:vue-standalone": "vue-cli-service build --target lib --name input --inline-vue --dest build/vue/standalone --formats umd-min src/vue/input.js"
  },
  "dependencies": {
    "lit": "^3.0.0-pre.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "parcel": "^2.9.3",
    "typescript": "~5.1.6"
  }
}
